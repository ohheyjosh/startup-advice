{
  "manifest_version": 2,

  "name": "Startup Advice",
  "description": "Startup tips on every new tab.",
  "version": "1.1.0",

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage"],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
